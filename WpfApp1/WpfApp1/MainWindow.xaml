<Window x:Class="ListBoxControl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:multibinding="clr-namespace:multiBindingNS"
        Title="MainWindow" Height="300" Width="525"
        x:Name="windowName">
    <StackPanel>
        <Label FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center" x:Name="zegar" ContentStringFormat="dd/MM (MMMM)/yyyy, dddd, H:mm:ss"/>
    </StackPanel>
</Window>
